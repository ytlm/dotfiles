Host *
    ControlMaster auto
    ControlPath ~/.ssh/connection-%r@%h:%p
    ControlPersist 4h
    ControlPersist yes
    Compression yes
    TCPKeepAlive yes
    ServerAliveInterval 30
    ServerAliveCountMax 6

Host ytl.debian
    HostName 10.23.179.64
    Port 22
    User root
    IdentityFile  ~/.ssh/id_rsa
    IdentitiesOnly yes
